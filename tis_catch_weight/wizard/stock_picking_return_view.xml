<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_stock_return_picking_form_cw_inherit" model="ir.ui.view">
        <field name="name">return.cw.quanity.inherit</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='to_refund']" position="before">
                <field name="catch_weight_ok" groups="tis_catch_weight.group_catch_weight" invisible="1"/>
                <field name="cw_quantity" attrs="{'readonly': [('catch_weight_ok', '=', True)]}"
                       groups="tis_catch_weight.group_catch_weight"/>
                <field name="cw_uom_id" attrs="{'readonly': [('catch_weight_ok', '=', True)]}"
                       groups="tis_catch_weight.group_catch_weight"/>
            </xpath>
        </field>
    </record>
</odoo>
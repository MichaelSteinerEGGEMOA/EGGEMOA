<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_stock_return_picking_form_cw_inherit" model="ir.ui.view">
        <field name="name">return.cw.quanity.inherit</field>
        <field name="model">stock.return.picking</field>
        <field name="inherit_id" ref="stock.view_stock_return_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='uom_id']" position="after">
                <field name="catch_weight_ok" invisible="1"/>
                <field name="cw_quantity" groups="tis_catch_weight.group_catch_weight"
                       attrs="{'readonly': [('catch_weight_ok', '=', False)]}"/>
                <field name="cw_uom_id" groups="tis_catch_weight.group_catch_weight"
                       readonly="1"/>
            </xpath>
        </field>
    </record>
</odoo>